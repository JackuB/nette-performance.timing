{**
 * performance.timing for Debugger Bar Panel (layout and JS)
 *
 * @author Jakub Mikuláš <jakub@jedenbod.cz>
 *
 *}

<script>
(function() {
    window.onload=function() {
        if (typeof performance != 'undefined') {
            setTimeout(function(){
                var perfStart = performance.timing.navigationStart;
                var perfReqStart = performance.timing.requestStart;
                var perfResStart = performance.timing.responseStart;
                var perfResEnd = performance.timing.responseEnd;
                var perfDomLoading = performance.timing.domLoading;
                var perfDomInter = performance.timing.domInteractive;
                var perfLoadStart = performance.timing.loadEventStart;
                var perfLoadEnd = performance.timing.loadEventEnd;

                var redirectTime = perfReqStart - perfStart;
                var requestTime = perfResStart - perfReqStart;
                var responseTime = perfResEnd - perfResStart;
                var domProcessingTime = perfDomInter - perfDomLoading;
                var domLoadingTime = perfLoadStart - perfDomInter;
                var loadEventTime = '';
                if(perfLoadEnd > 0) {
                    loadEventTime = perfLoadEnd - perfLoadStart;
                } else {
                    loadEventTime = 0;
                }

                var totalTime = redirectTime + requestTime + responseTime + domProcessingTime + domLoadingTime + loadEventTime;
                document.getElementById('NetteDebuggerBarPerformanceTiming').innerHTML = totalTime + 'ms';

                document.getElementById('NetteDebuggerBarPerformanceTimingredirectTime').innerHTML = redirectTime;
                document.getElementById('NetteDebuggerBarPerformanceTimingrequestTime').innerHTML = requestTime;
                document.getElementById('NetteDebuggerBarPerformanceTimingresponseTime').innerHTML = responseTime;
                document.getElementById('NetteDebuggerBarPerformanceTimingdomProcessingTime').innerHTML = domProcessingTime;
                document.getElementById('NetteDebuggerBarPerformanceTimingdomLoadingTime').innerHTML = domLoadingTime;
                document.getElementById('NetteDebuggerBarPerformanceTimingloadEventTime').innerHTML = loadEventTime;
                document.getElementById('NetteDebuggerBarPerformanceTimingtotalTime').innerHTML = totalTime;
            }, 20);
        }
    };
}());
</script>


<h1>
	<img src="data:image/png;base64," style="vertical-align: middle; position: relative; top: -4px; left: -2px;"/>
	performance.timing
</h1>
<div>
	<table>
		<tr>
			<th title="">redirectTime</th>
			<td style='text-align: right;'><div id="NetteDebuggerBarPerformanceTimingredirectTime"><em>Wait</em></div> ms</td>
		</tr>
        <tr>
            <th title="">requestTime</th>
            <td style='text-align: right;'><div id="NetteDebuggerBarPerformanceTimingrequestTime"><em>Wait</em></div> ms</td>
        </tr>
        <tr>
            <th title="">responseTime</th>
            <td style='text-align: right;'><div id="NetteDebuggerBarPerformanceTimingresponseTime"><em>Wait</em></div> ms</td>
        </tr>
        <tr>
            <th title="">domProcessingTime</th>
            <td style='text-align: right;'><div id="NetteDebuggerBarPerformanceTimingdomProcessingTime"><em>Wait</em></div> ms</td>
        </tr>
        <tr>
            <th title="">domLoadingTime</th>
            <td style='text-align: right;'><div id="NetteDebuggerBarPerformanceTimingdomLoadingTime"><em>Wait</em></div> ms</td>
        </tr>
        <tr>
            <th title="">loadEventTime</th>
            <td style='text-align: right;'><div id="NetteDebuggerBarPerformanceTimingloadEventTime"><em>Wait</em></div> ms</td>
        </tr>
		<tr>
			<th style='color: green;'>&sum;</th>
			<td style='color: green; text-align: right; border-top: 3px double #888;'><div id="NetteDebuggerBarPerformanceTimingtotalTime"><em>Wait</em></div> ms</td>
		</tr>
	</table>
</div>